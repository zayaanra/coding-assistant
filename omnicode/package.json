{
  "name": "omnicode",
  "displayName": "Omnicode",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "omnicode.codeCompletion",
        "title": "Omnicode - Code Completion"
      },
      {
        "command": "omnicode.refactorCode",
        "title": "Omnicode - Refactor Code"
      },
      {
        "command": "omnicode.generateDocumentation",
        "title": "Omnicode - Generate Documentation"
      },
      {
        "command": "omnicode.registerUser",
        "title": "Omnicode - Register"
      },
      {
        "command": "omnicode.loginUser",
        "title": "Omnicode - Login"
      },
      {
        "command": "omnicode.logoutUser",
        "title": "Omnicode - Logout"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "omnicode.codeCompletion",
          "group": "omnicode",
          "when": "true"
        },
        {
          "command": "omnicode.refactorCode",
          "group": "omnicode@1",
          "when": "editorHasSelection"
        },
        {
          "command": "omnicode.generateDocumentation",
          "group": "omnicode@2",
          "when": "editorHasSelection"
        },
        {
          "command": "omnicode.registerUser",
          "group": "omnicode@3",
          "when": "true"
        },
        {
          "command": "omnicode.loginUser",
          "group": "omnicode@4",
          "when": "true"
        },
        {
          "command": "omnicode.logoutUser",
          "group": "omnicode@5",
          "when": "true"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.693.0",
    "@aws-sdk/client-dynamodb": "^3.693.0",
    "dotenv": "^16.4.5"
  }
}
